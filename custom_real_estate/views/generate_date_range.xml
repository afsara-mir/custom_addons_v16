<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_date_list_form" model="ir.ui.view">
        <field name="name">date.list.form</field>
        <field name="model">date.list</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_date_list_tree" model="ir.ui.view">
        <field name="name">date.list.tree</field>
        <field name="model">date.list</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="start_date"/>
                <field name="end_date"/>
            </tree>
        </field>
    </record>
</odoo>